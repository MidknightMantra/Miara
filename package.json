{
   "name": "miara",
   "version": "1.7.4",
   "description": "Multi device WhatsApp bot with advanced features and plugin support",
   "main": "index.js",
   "type": "commonjs",
   "engines": {
      "node": ">=20.0.0",
      "npm": ">=10.0.0"
   },
   "scripts": {
      "start": "node .",
      "dev": "node --watch index.js",
      "pm2": "pm2 start index.js --name Miara --exp-backoff-restart-delay=100",
      "pm2:dev": "pm2 start index.js --name Miara --watch --ignore-watch='node_modules temp'",
      "pm2:monitor": "pm2 start index.js --deep-monitoring --attach --name Miara",
      "stop": "pm2 stop Miara",
      "restart": "pm2 restart Miara",
      "delete": "pm2 delete Miara",
      "logs": "pm2 logs Miara --lines 100",
      "status": "pm2 status",
      "kill": "pm2 kill",
      "postinstall": "npx puppeteer browsers install chrome"
   },
   "keywords": [
      "whatsapp",
      "bot",
      "whatsapp-bot",
      "baileys",
      "multi-device",
      "automation",
      "messaging"
   ],
   "author": "Miara Bot Team",
   "license": "Apache-2.0",
   "dependencies": {
      "@adiwajshing/keyed-db": "^0.2.4",
      "@hapi/boom": "^10.0.1",
      "@whiskeysockets/baileys": "^6.7.9",
      "acrcloud": "^1.4.0",
      "anime-character-random": "latest",
      "aptoide-scraper": "latest",
      "async-g-i-s": "^1.4.0",
      "axios": "^1.7.7",
      "canvas": "^2.11.2",
      "cheerio": "^1.0.0",
      "child_process": "^1.0.2",
      "crypto-js": "^4.2.0",
      "discord-mongoose-economy": "^1.2.0",
      "discord-xp": "^1.1.16",
      "dotenv": "^16.4.5",
      "express": "^4.21.1",
      "ffmpeg": "^0.0.4",
      "file-type": "^16.5.4",
      "fluent-ffmpeg": "^2.1.3",
      "form-data": "^4.0.1",
      "fs-extra": "^11.2.0",
      "google-it": "^1.6.4",
      "google-tts-api": "^2.0.2",
      "heroku-client": "^3.1.0",
      "human-readable": "^0.2.1",
      "jimp": "^1.6.0",
      "jsdom": "^25.0.1",
      "koyeb-api-client": "latest",
      "link-preview-js": "^3.1.0",
      "moment-timezone": "^0.5.46",
      "mongoose": "^8.8.1",
      "mumaker": "^2.0.0",
      "node-cron": "^3.0.3",
      "node-fetch": "^2.7.0",
      "node-webpmux": "^3.1.7",
      "pastebin-js": "latest",
      "path": "^0.12.7",
      "pg": "^8.13.1",
      "pm2": "^5.4.2",
      "puppeteer": "latest",
      "qrcode": "latest",
      "ruhend-scraper": "^8.0.3",
      "secktor-pack": "latest",
      "simple-git": "^3.27.0",
      "translatte": "^3.0.1",
      "util": "^0.12.5",
      "wa-sticker-formatter": "^4.4.4",
      "youtubei.js": "^10.5.0",
      "ytdl-secktor": "latest"
   },
   "overrides": {
      "ytdl-secktor": "npm:@distube/ytdl-core"
   },
   "directories": {
      "lib": "lib",
      "plugins": "plugins"
   },
   "devDependencies": {
      "pino": "^7.0.5"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/MidknightMantra/Miara.git"
   },
   "bugs": {
      "url": "https://github.com/MidknightMantra/Miara/issues"
   },
   "homepage": "https://github.com/MidknightMantra/Miara#readme"
}
